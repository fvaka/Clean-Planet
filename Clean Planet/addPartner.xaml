<Window x:Class="Clean_Planet.addPartner"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Clean_Planet"
        mc:Ignorable="d"
        Title="Добавление партнера" Height="439" Width="351">
    <Grid Background="LightCyan" Margin="0,0,0,-6">
        <Label Content="Наименование" HorizontalAlignment="Left" Margin="66,26,0,0" VerticalAlignment="Top" FontFamily="Franklin Gothic Medium"/>
        <Label Content="Тип партнера" HorizontalAlignment="Left" Margin="69,57,0,0" VerticalAlignment="Top" FontFamily="Franklin Gothic Medium"/>
        <Label Content="Рейтинг" HorizontalAlignment="Left" Margin="93,93,0,0" VerticalAlignment="Top" FontFamily="Franklin Gothic Medium"/>
        <Label Content="Адрес" HorizontalAlignment="Left" Margin="101,124,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.232,0.587" FontFamily="Franklin Gothic Medium"/>
        <Label Content="ФИО директора" HorizontalAlignment="Left" Margin="48,162,0,0" VerticalAlignment="Top" FontFamily="Franklin Gothic Medium"/>
        <Label x:Name="innLB" Content="ИНН" HorizontalAlignment="Left" Margin="104,204,0,0" VerticalAlignment="Top" FontFamily="Franklin Gothic Medium"/>
        <Label Content="Телефон" HorizontalAlignment="Left" Margin="84,240,0,0" VerticalAlignment="Top" FontFamily="Franklin Gothic Medium"/>
        <Label Content="Email" HorizontalAlignment="Left" Margin="102,273,0,0" VerticalAlignment="Top" FontFamily="Franklin Gothic Medium"/>
        <Button x:Name="addBtn" Content="Добавить" HorizontalAlignment="Left" Margin="134,315,0,0" VerticalAlignment="Top" Height="29" Width="90" Click="addBtn_Click" BorderBrush="#FF67BB80" Background="DarkTurquoise" FontFamily="Franklin Gothic Medium"/>
        <Button x:Name="returnBtn" Content="Назад" HorizontalAlignment="Left" Margin="154,359,0,0" VerticalAlignment="Top" ContentStringFormat="" Click="returnBtn_Click" Height="22" Width="48" Background="#FFCBD6E5" FontFamily="Franklin Gothic Medium"/>
        <TextBox x:Name="nameTB" HorizontalAlignment="Left" Margin="195,30,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" FontFamily="Franklin Gothic Medium"/>
        <TextBox x:Name="ratingTB" HorizontalAlignment="Left" Margin="195,93,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="122" Height="22" FontFamily="Franklin Gothic Medium"/>
        <TextBox x:Name="addressTB" HorizontalAlignment="Left" Margin="192,124,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="123" Height="21" FontFamily="Franklin Gothic Medium"/>
        <TextBox x:Name="FIODTb" HorizontalAlignment="Left" Margin="193,162,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="123" Height="26" FontFamily="Franklin Gothic Medium"/>
        <TextBox x:Name="InnTB" HorizontalAlignment="Left" Margin="193,204,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="123" Height="26" FontFamily="Franklin Gothic Medium"/>
        <TextBox x:Name="phoneTB" HorizontalAlignment="Left" Margin="192,242,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="124" Height="22" FontFamily="Franklin Gothic Medium"/>
        <TextBox x:Name="emailTb" HorizontalAlignment="Left" Margin="192,277,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="122" Height="22" FontFamily="Franklin Gothic Medium"/>
        <ComboBox x:Name="typeCMB" HorizontalAlignment="Left" Margin="193,61,0,0" VerticalAlignment="Top" Width="120" FontFamily="Franklin Gothic Medium">
            <ComboBox.Background>
                <SolidColorBrush Color="White"/>
            </ComboBox.Background>
        </ComboBox>
    </Grid>
</Window>
