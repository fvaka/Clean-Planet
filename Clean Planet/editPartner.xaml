<Window x:Class="Clean_Planet.editPartner"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Clean_Planet"
        mc:Ignorable="d"
        Title="Редактирование партнера" Height="411" Width="351">
    <Grid Background="LightCyan" Margin="0,-15,-3,-6">
        <Label Content="Наименование" HorizontalAlignment="Left" Margin="62,28,0,0" VerticalAlignment="Top" FontFamily="Franklin Gothic Medium"/>
        <Label Content="Тип партнера" HorizontalAlignment="Left" Margin="65,59,0,0" VerticalAlignment="Top" FontFamily="Franklin Gothic Medium"/>
        <Label Content="Рейтинг" HorizontalAlignment="Left" Margin="89,95,0,0" VerticalAlignment="Top" FontFamily="Franklin Gothic Medium"/>
        <Label Content="Адрес" HorizontalAlignment="Left" Margin="97,126,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.232,0.587" FontFamily="Franklin Gothic Medium"/>
        <Label Content="ФИО директора" HorizontalAlignment="Left" Margin="44,164,0,0" VerticalAlignment="Top" FontFamily="Franklin Gothic Medium"/>
        <Label x:Name="innLB" Content="ИНН" HorizontalAlignment="Left" Margin="97,201,0,0" VerticalAlignment="Top" FontFamily="Franklin Gothic Medium"/>
        <Label Content="Телефон" HorizontalAlignment="Left" Margin="80,236,0,0" VerticalAlignment="Top" FontFamily="Franklin Gothic Medium"/>
        <Label Content="Email" HorizontalAlignment="Left" Margin="97,271,0,0" VerticalAlignment="Top" FontFamily="Franklin Gothic Medium"/>
        <Button x:Name="saveBtn" Content="Сохранить" HorizontalAlignment="Left" Margin="133,310,0,0" VerticalAlignment="Top" Height="29" Width="90" Click="saveBtn_Click" BorderBrush="#FF67BB80" Background="DarkTurquoise" FontFamily="Franklin Gothic Medium"/>
        <Button x:Name="returnBtn" Content="Назад" HorizontalAlignment="Left" Margin="154,354,0,0" VerticalAlignment="Top" ContentStringFormat="" Click="returnBtn_Click" Height="22" Width="48" Background="#FFCBD6E5" FontFamily="Franklin Gothic Medium"/>
        <TextBox x:Name="nameTB" HorizontalAlignment="Left" Margin="191,32,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" FontFamily="Franklin Gothic Medium"/>
        <TextBox x:Name="ratingTB" HorizontalAlignment="Left" Margin="191,95,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="122" Height="22" FontFamily="Franklin Gothic Medium"/>
        <TextBox x:Name="addressTB" HorizontalAlignment="Left" Margin="188,126,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="123" Height="21" FontFamily="Franklin Gothic Medium"/>
        <TextBox x:Name="FIODTb" HorizontalAlignment="Left" Margin="189,164,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="123" Height="26" FontFamily="Franklin Gothic Medium"/>
        <TextBox x:Name="InnTB" HorizontalAlignment="Left" Margin="190,201,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="123" Height="26" FontFamily="Franklin Gothic Medium"/>
        <TextBox x:Name="phoneTB" HorizontalAlignment="Left" Margin="191,240,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="123" Height="22" FontFamily="Franklin Gothic Medium"/>
        <TextBox x:Name="emailTb" HorizontalAlignment="Left" Margin="191,273,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="123" Height="22" FontFamily="Franklin Gothic Medium"/>
        <ComboBox x:Name="typeCMB" HorizontalAlignment="Left" Margin="189,63,0,0" VerticalAlignment="Top" Width="120" FontFamily="Franklin Gothic Medium">
            <ComboBox.Background>
                <SolidColorBrush Color="White"/>
            </ComboBox.Background>
        </ComboBox>
    </Grid>
</Window>
